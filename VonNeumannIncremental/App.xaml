<Application x:Class="VonNeumannIncremental.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:st1="clr-namespace:VonNeumannIncremental.Stages.Stage1"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type st1:Stage1ViewModel}">
                <st1:Stage1View/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type st1:PartsConstructionViewModel}">
                <st1:PartsConstructionView/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type st1:ProbeAssemblyViewModel}">
                <st1:ProbeAssemblyView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
